{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/website/$1"
    }
  ],
  "outputDirectory": "website",
  "env": {
    "NODE_ENV": "production"
  }
}
